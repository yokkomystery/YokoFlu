# Staging configuration
PRODUCT_BUNDLE_IDENTIFIER = {{BUNDLE_ID}}.staging
PRODUCT_NAME = {{APP_NAME}} Staging
DISPLAY_NAME = {{APP_NAME}} Staging
CODE_SIGN_STYLE = Automatic
DEVELOPMENT_TEAM = 
PROVISIONING_PROFILE_SPECIFIER = 
CODE_SIGN_IDENTITY = iPhone Developer

# Firebase configuration
FIREBASE_CONFIG_FILE = GoogleService-Info-staging.plist

# Build settings
SWIFT_VERSION = 5.0
IPHONEOS_DEPLOYMENT_TARGET = 13.0
ENABLE_BITCODE = NO
VALIDATE_WORKSPACE = YES

# Staging flags
DEBUG_INFORMATION_FORMAT = dwarf-with-dsym
ONLY_ACTIVE_ARCH = YES
GCC_OPTIMIZATION_LEVEL = 0
GCC_PREPROCESSOR_DEFINITIONS = STAGING=1 COCOAPODS=1 